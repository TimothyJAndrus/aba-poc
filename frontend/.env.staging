# Staging Environment Configuration

# API Configuration
VITE_API_BASE_URL=https://staging-api.aba-scheduling.com/api
VITE_WS_URL=wss://staging-api.aba-scheduling.com

# Application Configuration
VITE_APP_NAME=ABA Scheduling System (Staging)
VITE_APP_VERSION=1.0.0-staging
VITE_ENVIRONMENT=staging

# Sentry Configuration (replace with actual values)
VITE_SENTRY_DSN=https://your-staging-sentry-dsn@sentry.io/project-id
VITE_SENTRY_ORG=your-org
VITE_SENTRY_PROJECT=aba-scheduling-ui-staging
VITE_SENTRY_RELEASE=aba-scheduling-ui@1.0.0-staging

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Security
VITE_ENABLE_CSP=true
VITE_ENABLE_HTTPS_ONLY=true

# Performance
VITE_ENABLE_SERVICE_WORKER=true
VITE_ENABLE_CODE_SPLITTING=true
VITE_ENABLE_LAZY_LOADING=true

# Monitoring
VITE_LOG_LEVEL=warn
VITE_ENABLE_DEBUG=true